# Обязательные переменные
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
YANDEX_CLOUD_CATALOG_ID=your_catalog_id_here

# Тип аутентификации (static_token, service_account, oauth)
YANDEX_AUTH_TYPE=static_token

# Для static_token аутентификации
YANDEX_CLOUD_TOKEN=your_static_token_here

# Для service_account аутентификации
# YANDEX_SERVICE_ACCOUNT_KEY_FILE=/path/to/service-account-key.json
# или
# YANDEX_SERVICE_ACCOUNT_KEY_JSON={"id":"...","service_account_id":"...","private_key":"..."}

# Для oauth аутентификации
# YANDEX_OAUTH_TOKEN=your_oauth_token_here
